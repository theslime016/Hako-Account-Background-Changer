{
  "manifest_version": 3,
  "name": "Hako Account Background Changer",
  "version": "1.0",
  "description": "Thay đổi background cho trang account hako",
  "permissions": ["activeTab", "storage"],
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["https://docln.net/*", "https://ln.hako.vn/*"],
      "js": ["script.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
